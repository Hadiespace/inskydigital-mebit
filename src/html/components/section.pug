section.section
	div.section__top-wrapper.container
		div.section__top-content
			h1.section__top-title Спальни
			button.section__top-button(type="button", aria-expanded="false")
				svg(width="18", height="15", aria-hidden="true")
					use(xlink:href="images/sprite.svg#filters-open")
				span Показать фильтры
		ul.section__top-list
			each link in sectionLinks
				li.section__top-item
					button.section__top-link(type="button")!= link.text
			li.section__top-item
				button.favorites__top-button.cost(type="button")
					span.cost__value.cost__value--current ₽
					span.cost__value €
	div.section__top-filters
		div.section__top-filter-container.container
			div.section__top-filter
				form.section__top-form.range.range--cost
					p.range__title Цена
					div.range__slider
					div.range__values
						span.range__val ₽
						p.range__value.range__value--min
							input.range__input(type="text", id="range10", min="0", value="98000")
							label.visually-hidden(for="range10") Минимальная цена
						p.range__value.range__value--max
							input.range__input(type="text", id="range11", max="1000000", value="743000")
							label.visually-hidden(for="range11") Максимальная цена
				+choicePopup
			div.section__top-filter
				p.section__top-filter-title Фабрика
				+choiceCheck('Все фабрики', 'factory', sectionChoiceItems)
				+choicePopup
			div.section__top-filter
				p.section__top-filter-title Модель
				+choiceCheck('Все модели', 'model', sectionChoiceItems)
				+choicePopup
			div.section__top-filter
				p.section__top-filter-title Материал
				+choiceCheck('Все материалы', 'material', sectionMaterialItems)
				+choicePopup
			div.section__top-filter
				p.section__top-filter-title Наличие
				+choiceCheck('Все товары', 'status', sectionStatusItems)
				+choicePopup
			div.section__top-filter
				p.section__top-filter-title Стиль
				+choiceCheck('Все стили', 'status', sectionStyleItems)
				+choicePopup
	ul.section__top-result.container
			li.section__top-result-item
				span Цена: от 50000₽ до 70000₽
				button(type="button")
					span.visually-hidden Удалить фильтр
					svg(width="10", height="10", aria-hidden="true")
						use(xlink:href="images/sprite.svg#close")
			li.section__top-result-item
				span Фабрика: Modum, Camelgroup
				button(type="button")
					span.visually-hidden Удалить фильтр
					svg(width="10", height="10", aria-hidden="true")
						use(xlink:href="images/sprite.svg#close")
			li.section__top-result-item
				span Фабрика: Modum, Camelgroup
				button(type="button")
					span.visually-hidden Удалить фильтр
					svg(width="10", height="10", aria-hidden="true")
						use(xlink:href="images/sprite.svg#close")
			li.section__top-result-item
				span Модель: Platinum, Alchemy, Infinity
				button(type="button")
					span.visually-hidden Удалить фильтр
					svg(width="10", height="10", aria-hidden="true")
						use(xlink:href="images/sprite.svg#close")
	div.section__list-wrapper
		div.section__container.container
			ul.section__list
				each item in sectionItems
					// Товар: набор
					li.section__item.card.card--set
						div.card__tags-wrapper
							div.card__tags
								if item.tags
									each tag in item.tags
										span!= tag
						// Стоковое состояние
						div.card__stock.stock-card
							h2.stock-card__title!= item.title
							span.stock-card__cost.stock-card__cost от #{item.costRu}
							div.stock-card__image
								img(src=item.image.src, alt=item.image.alt)
								button.stock-card__favorite(type="button")
									span.visually-hidden Добавить в избранное
									svg(width="16", height="14", aria-hidden="true")
										use(xlink:href="images/sprite.svg#favorite")
						// Активное состояние
						div.card__active.active-card
							a.active-card__link(href=item.href)
								span.visually-hidden Перейти на страницу с товаром
							div.card__all-wrapper
								div.card__tags
									if item.tags
										each tag in item.tags
											span!= tag
								button.card__favorite(type="button")
									span.visually-hidden Добавить в избранное
									svg(width="16", height="14", aria-hidden="true")
										use(xlink:href="images/sprite.svg#favorite")
							div.active-card__swiper-wrapper
								div.active-card__swiper.swiper
									ul.active-card__swiper-list.swiper-wrapper
										li.active-card__swiper-item.swiper-slide
											img(src=item.image.src, alt=item.image.alt)
										li.active-card__swiper-item.swiper-slide
											img(src=item.image.src, alt=item.image.alt)
								div.active-card__swiper-dots
							div.active-card__content-wrapper
								p.active-card__title!= item.title
								p.active-card__element
									span Фабрика:
									a(href=index)!= item.factory
								p.active-card__element
									span Коллекция:
									a(href=index)!= item.collection
								div.active-card__wrapper
									p.active-card__cost Цена от #{item.costRu}
									p.active-card__status В наличии
								a.active-card__button Оставить заявку —
								a.active-card__preview-open Быстрый просмотр
			+pagination.section__pagination
